<!DOCTYPE html>

<head>
  <script type="text/javascript" src="../libs/jquery.js"></script>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
  <link rel="stylesheet" href="./public/css/nav.css">
  <link rel="stylesheet" href="./public/css/ranking.css">
  <script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
  <script src="http://netdna.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
</head>

<body>
  <%- include('./nav.ejs') %>
  <h1>Bizus pra resolverem sua vida:</h1>
  
  <ul>
   <% arrayBizus.forEach (bizu => { %>
      <li>
        <h3>Título: <%= bizu.title %></h3>
        <h3>Matéria: <%= bizu.subject %></h3>
        <!-- <h3>Creator: <%= bizu.creator %></h3> --> 
        <h3>Conteúdo:</h3>
        <h4><%= bizu.text %></h4> 
        <button class="someButton btn btn-primary" type="button" onclick="location.href = '/rankbizu/<%= bizu._id %>'" >Dê uma nota para este bizu!</button>
      </li>  

    <% } ) %>
  </ul>
</body>

</html>